{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "Ultreon Devices Mod",
      "description": "This is an example description! Tell everyone what your mod is about!",
      "contributors": {
        "Me!": "Author"
      },
      "icon": "assets/devices/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "dev.ultreon.devices.quilt.DevicesQuiltMod"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.2"
      },
      {
        "id": "minecraft",
        "version": ">=1.21"
      }
    ]
  },
  "mixin": [
    "devices.quilt.mixins.json",
    "devices.mixins.json"
  ]
}
